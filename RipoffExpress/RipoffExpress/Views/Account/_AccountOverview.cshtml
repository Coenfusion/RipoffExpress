@using RipoffExpress.Models.AccountModels;

@model AccountDetails

    <table>
        <tr><td>Username:   </td><td> @Model.Username  </td></tr>
        <tr><td>E-mail:     </td><td> @Model.Email     </td></tr>
        <tr><td>Phone:      </td><td> @Model.ShippingAddress.PhoneNumber  </td></tr>
    </table>
    <table>
        <thead>Shipping Address</thead>
        <tr><td>Address:        </td><td> @Model.ShippingAddress.Address    </td></tr>
        <tr><td>Postal Code:    </td><td> @Model.ShippingAddress.PostalCode </td></tr>
        <tr><td>City:           </td><td> @Model.ShippingAddress.City       </td></tr>
        <tr><td>Province:       </td><td> @Model.ShippingAddress.Province   </td></tr>
        <tr><td>Country:        </td><td> @Model.ShippingAddress.Country    </td></tr>
    </table>




