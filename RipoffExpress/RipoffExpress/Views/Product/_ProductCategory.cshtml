@using RipoffExpress.Models;

@model IEnumerable<Category>

@{
    ViewData["Title"] = "RipOffExpress";
}

<h4>Categories</h4>
<table>
    @foreach (Category Category in Model)
    {
        <tr>
            <td>
                <a value="@Category.Name" onclick="ShowProducts(@Category.Id)">@Category.Name</a>
            </td>
        </tr>
    }
</table>